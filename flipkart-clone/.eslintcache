yData = useSelector(state => state.categoryData)\r\n    const dispatch = useDispatch()\r\n    useEffect(() => {\r\n        dispatch(getAllCategories());\r\n    }, [])\r\n\r\n    //递归算法,将category放进li\r\n    const renderCategory=(categoriesData)=>{\r\n        let categoryList=[];\r\n        categoriesData.map(category=>\r\n            categoryList.push(\r\n                <li key={category._id}>\r\n                    {\r\n                        //如果没有parentId，点击之后画面就不会迁移\r\n                        category.parentId ? <a href={`/${category.slug}?cid=${category._id}&type=${category.type}`}>{category.name}</a> : <span>{ category.name}</span>\r\n                    }\r\n                    {category.children.length>0?(<ul>{renderCategory(category.children)}</ul>):null}\r\n                </li>\r\n            )\r\n        )\r\n        return  categoryList\r\n    }\r\n\r\n    return (\r\n        <div className=\"menuHeader\">\r\n            <ul>\r\n                {categoryData.categories.length>0?renderCategory(categoryData.categories):null}\r\n            </ul>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default MenuHeader\r\n","C:\\Users\\zhang\\Desktop\\20200930-flipkart-clone\\flipkart-clone\\src\\redux\\category\\categoryAction.js",["146"],"import axios from \"axios\"\r\nimport {categoryConstants} from \"../actionTypes.js\"\r\nexport const getAllCategories=()=>{\r\n    return async(dispatch)=>{\r\n        dispatch({type:categoryConstants.GET_ALL_CATEGORIES_REQUEST})\r\n        const response=await axios.get(\"/api/category/getCategories\")\r\n        if(response.status==200){\r\n            const categoryList=response.data.data\r\n            dispatch({\r\n                type:categoryConstants.GET_ALL_CATEGORIES_SUCCESS,\r\n                payload:{categories:categoryList},\r\n            })\r\n        }else{\r\n            dispatch({\r\n                type:categoryConstants.GET_ALL_CATEGORIES_FAILED,\r\n                payload:{error:response.data.error} //这个er